import extensions.

ackermann(m,n)
[
    if((n < 0)||(m < 0))
    [
        InvalidArgumentException new; raise
    ].

    m =>
       0 [ ^n + 1 ];
       ! [
              n =>
                0 [ ^ackermann(m - 1,1) ];
                ! [ ^ackermann(m - 1,ackermann(m,n-1)) ]
           ]
]

public program
[
    0 to:3 do(:i)
    [
        0 to:5 do(:j)
        [
            console printLine("A(",i,",",j,")=",ackermann(i,j))
        ]
    ].

    console readChar
]
