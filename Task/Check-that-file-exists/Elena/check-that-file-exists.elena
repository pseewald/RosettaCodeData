import system'io.
import extensions.

extension op
{
    validatePath
        = self  isAvailable; iif("exists","not found").
}

public program =
[
    console printLine("input.txt file ",File new("input.txt"); validatePath).

    console printLine("\input.txt file ",File new("\input.txt"); validatePath).

    console printLine("docs directory ",Directory new("docs"); validatePath).

    console printLine("\docs directory ",Directory new("\docs"); validatePath).
].
