import extensions.

class MyClass
{
    object prop Variable :: _variable.

    someMethod
    [
        _variable := 1.
    ]

    constructor new
    [
    ]
}

public program =
[
    // instantiate the class
    var instance := MyClass new.

    // invoke the method
    instance someMethod.

    // get the variable
    console printLine("Variable=",instance Variable).
].
